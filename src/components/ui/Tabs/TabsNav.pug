// -
    param {
        items: [{label, targetId}]
    }

mixin TabsNav(param = {})

    - param.items = param.items || [{ label: 'test 1', targetId: 'test2' }, { label: 'test 2', targetId: 'test2' }]

    .tabs-img
      .tabs-car 
        img(src="/assets/img/categories/vnd/car.png", alt="")
      .tabs-mark 
        img(src="/assets/img/categories/vnd/mark.png", alt="")


    .tabs-nav

        - for (let i = 0; i < param.items.length; i++)

            button.tabs-nav__btn(
                class=i==0 ? 'active' : ''
                data-tab-target=param.items[i].targetId
            ) 
              +icon('icon-' + param.items[i].svg)
              span !{param.items[i].label}
